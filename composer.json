{
    "name": "requtize/semver-converter",
    "type": "library",
    "keywords": [ "semver", "semantic version", "converter", "php", "semver converter", "semver to int", "semver to integer" ],
    "description": "Converts SemVer version (like Composer packages) into integer version with operators. Helps managing versions: store, compare, sort and retrive by conditions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Banaszkiewicz",
            "homepage": "http://adambanaszkiewicz.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5 || ~7.0 || ~8.0",
        "composer/semver": "^3.2"
    },
    "autoload": {
        "psr-4": { "Requtize\\SemVerConverter\\": "src/SemVerConverter/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
